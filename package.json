{
  "name": "tmux-kill",
  "version": "1.0.0",
  "description": "Interactively kill a tmux session",
  "homepage": "https://github.com/ruyadorno/tmux-kill",
  "author": {
    "name": "Ruy Adorno",
    "url": "http://ruyadorno.com"
  },
  "bin": "cli.js",
  "files": [
    "cli.js"
  ],
  "keywords": [
    "tmux",
    "kill",
    "interactive",
    "ipt",
    "alias"
  ],
  "devDependencies": {
    "eslint": "^5.4.0",
    "eslint-config-prettier": "^3.0.1",
    "eslint-config-xo": "^0.25.0",
    "eslint-plugin-prettier": "^3.0.0",
    "husky": "^1.1.3",
    "jest": "^23.5.0",
    "lint-staged": "^8.0.4",
    "prettier": "^1.11.1"
  },
  "engines": {
    "npm": ">= 4.0.0"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "prettier"
    ],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "printWidth": 80
        }
      ]
    },
    "plugins": [
      "prettier"
    ]
  },
  "scripts": {
    "pretest": "eslint .",
    "precommit": "lint-staged",
    "test": "jest"
  },
  "repository": "ruyadorno/tmux-kill",
  "jest": {
    "testEnvironment": "node"
  },
  "license": "MIT",
  "dependencies": {
    "ipt": "^2.1.0"
  }
}
